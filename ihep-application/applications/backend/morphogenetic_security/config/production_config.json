{
  "engine": {
    "name": "IHEP Morphogenetic Security",
    "version": "1.0.0",
    "environment": "production"
  },
  "security": {
    "zero_trust": true,
    "byzantine_fault_tolerance": true,
    "consensus_threshold": 0.75,
    "encryption_at_rest": true,
    "encryption_in_transit": true,
    "min_agent_consensus": 3
  },
  "performance": {
    "max_events_per_second": 10000,
    "max_threads": 100,
    "event_queue_size": 50000,
    "response_time_target_ms": 50
  },
  "evolution": {
    "enabled": true,
    "min_evolution_interval_hours": 24,
    "architecture_mutation_rate": 0.1,
    "threat_threshold_for_evolution": 100
  },
  "storage": {
    "base_path": "/var/ihep/morphogenetic_security",
    "database_path": "/var/ihep/morphogenetic_security/fragmentation.db",
    "retention_policy": "INDEFINITE",
    "compression": true,
    "encryption": "AES-256-GCM"
  },
  "fragmentation_database": {
    "encryption_enabled": true,
    "zero_trust_access": true,
    "audit_all_access": true
  },
  "procedural_registry": {
    "retention_policies": {
      "hipaa_days": 2190,
      "pci_days": 365,
      "default_days": 3650
    },
    "synergy_threshold": 0.7,
    "storage_quota_gb": 1000,
    "storage_critical_threshold": 0.85,
    "required_approvers": [
      "security_lead",
      "compliance_officer",
      "data_scientist"
    ]
  },
  "threat_intelligence": {
    "mitre_attack": {
      "enabled": true,
      "cache_dir": "/var/ihep/mitre_attack",
      "update_interval_days": 7
    },
    "rapid7": {
      "enabled": true,
      "api_key_env": "RAPID7_API_KEY",
      "cache_dir": "/var/ihep/rapid7",
      "ip_reputation_enabled": true
    },
    "nvd_cve": {
      "enabled": true,
      "api_key_env": "NVD_API_KEY",
      "cache_dir": "/var/ihep/nvd_cve",
      "scan_interval_hours": 6
    }
  },
  "agents": {
    "byzantine_consensus_threshold": 3,
    "heartbeat_interval_seconds": 1,
    "health_check_interval_seconds": 5,
    "agent_timeout_seconds": 10
  },
  "sandbox": {
    "enabled": true,
    "network_isolation": true,
    "max_concurrent_sandboxes": 50,
    "sandbox_lifetime_minutes": 30,
    "intelligence_extraction_enabled": true,
    "synthetic_phi_count": 1000
  },
  "osi_layers": {
    "layer_1_physical": {
      "enabled": true,
      "agents_per_quadrant": 4
    },
    "layer_2_data_link": {
      "enabled": true,
      "agents_per_quadrant": 4
    },
    "layer_3_network": {
      "enabled": true,
      "agents_per_quadrant": 4,
      "firewall_integration": true
    },
    "layer_4_transport": {
      "enabled": true,
      "agents_per_quadrant": 4,
      "rate_limiting_enabled": true
    },
    "layer_5_6_session": {
      "enabled": true,
      "agents_per_quadrant": 4,
      "csrf_protection": true
    },
    "layer_7_application": {
      "enabled": true,
      "agents_per_quadrant": 4,
      "waf_enabled": true,
      "input_validation": true,
      "output_encoding": true
    }
  },
  "integration": {
    "ihep_security_modules": {
      "clinical_input_validator": true,
      "phi_output_encoder": true,
      "inter_system_security": true,
      "hipaa_audit_logger": true,
      "envelope_encryption": true
    }
  },
  "logging": {
    "level": "INFO",
    "file": "/var/log/ihep/morphogenetic_security.log",
    "max_size_mb": 100,
    "backup_count": 10,
    "audit_log_file": "/var/log/ihep/security_audit.log"
  }
}
